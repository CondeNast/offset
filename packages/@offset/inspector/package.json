    "@atjson/hir": "^0.21.12",
    "@atjson/renderer-commonmark": "0.21.20",
    "@atjson/renderer-hir": "0.21.12",
    "@atjson/renderer-html": "0.23.3",
    "@atjson/renderer-react": "0.22.0",
    "@atjson/source-html": "0.25.3",
    "@tce/rush": "0.6.5",
    "@types/dom-inputevent": "^1.0.1",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.8.5",
    "@types/styled-components": "^4.1.18",
    "node-sass": "^4.9.0",
    "parcel": "^1.12.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
