{
  "name": "@offset/inspector",
  "version": "0.1.0",
  "description": "Offset Inspector",
  "main": "dist/commonjs/index.js",
  "module": "dist/modules/index.js",
  "types": "dist/commonjs/index.d.ts",
  "scripts": {
    "build": "rm -rf dist; tsc -p . & tsc -p . --module ESNext --outDir dist/modules/ --target ES2017; exit 0",
    "lint": "tslint -c ./tslint.json 'src/**/*.ts'",
    "prepublishOnly": "npm run build",
    "start": "parcel index.html",
    "test": "../../../node_modules/.bin/jest packages/@offset/$(basename $PWD) --config=./jest.config.js"
  },
  "license": "Apache-2.0",
  "publishConfig": {
